{
  "name": "@lollygag/markdown",
  "version": "0.0.1",
  "description": "Markdown worker for Lollygag.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "John Romeral",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jegwastaken/lollygag.git",
    "directory": "packages/markdown"
  },
  "scripts": {
    "build": "rimraf dist && tsc -d",
    "start": "rimraf dist && tsc -d -w",
    "eslint:panadero": "npx install-peerdeps --dev eslint-config-panadero",
    "prettier": "prettier --write .",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "yarn prettier && yarn eslint"
  },
  "dependencies": {
    "@lollygag/core": "^0.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.31",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-panadero": "0.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
